{
    "name": "smacon/broucher",
    "description": "Broucher package for smacon.",
    "keywords": ["lavalite", "Broucher"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Smacon\\Broucher\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smacon\\Broucher\\Providers\\BroucherServiceProvider"
            ],
            "aliases": {
                "Blog"       : "Smacon\\Broucher\\Facades\\Broucher"
            }
        }
    },
    "minimum-stability": "dev"
}
